{
  "actions": {
    "initial": {
      "method": "GET",
      "url": "http://petstore.swagger.io/v2/pet/findByStatus",
      "params": "json/pets_list_request_params.json"
    }
  },
  "type": "table",
  "form": {
    "tableKeys": [
      {
        "title": "ID",
        "bind": "id"
      },
      {
        "title": "Name",
        "bind": "name"
      },
      {
        "title": "Is available?",
        "bind": "status",
        "value-map": {
          "available": "yes",
          "notavailable": "no"
        }
      }
    ]
  }
}
